{
    "check-coverage": true,
    "branches": 0,
    "lines": 80,
    "functions": 0,
    "statements": 0,
    "all": true,
  "include": ["src/**/*.{ts,tsx,js,jsx}"],
  "exclude": ["node_modules", "tests", "dist"],
  "reporter": ["text", "json", "html", "json-summary"],
  "report-dir": "./coverage"
}